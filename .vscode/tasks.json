{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "清理",
            "type": "shell",
            "command": "${workspaceFolder}\\clean.cmd",
            "problemMatcher": []
        },
        {
            "label": "构建",
            "type": "shell",
            "command": "${workspaceFolder}\\build.cmd",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
}
